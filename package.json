{
  "name": "knowledge_extractor",
  "version": "1.0.0",
  "main": "server/index.js",
  "description": "A knowledge extraction and organization platform",
  "scripts": {
    "dev": "nodemon server/index.js",
    "start": "node server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-ffmpeg": "npm install ffmpeg-static --save-optional"
  },
  "keywords": [
    "knowledge",
    "extraction",
    "organization"
  ],
  "author": "",
  "license": "ISC",

  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "yt-dlp-exec": "^1.0.0",
    "openai": "^4.52.0",
    "fluent-ffmpeg": "^2.1.3"
  },
  "optionalDependencies": {
    "puppeteer": "^21.6.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "ffmpeg-static": "^5.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "node-fetch": "^2.7.0"
  },
  "packageManager": "npm@9.0.0",
  "notes": "系统依赖：需要在Railway上安装ffmpeg和yt-dlp。在Dockerfile中添加: RUN apt-get update && apt-get install -y ffmpeg yt-dlp",
  "overrides": {
    "puppeteer": "$puppeteer"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
